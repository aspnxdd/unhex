{
  "name": "unhex",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "package/*",
    "docs/*"
  ],
  "scripts": {
    "turbo:build": "turbo run build",
    "package:compile": "pnpm --filter package run compile",
    "package:test": "pnpm --filter package run test",
    "package:publish": "pnpm --filter package publish",
    "docs:build": "pnpm --filter docs run build"
  },
  "devDependencies": {
    "turbo": "^1.5.3",
    "prettier": "^2.3.2"
  }
}